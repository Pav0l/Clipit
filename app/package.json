{
  "name": "clip-it",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "app:start:dev": "NODE_ENV=development webpack serve --mode development",
    "app:build:prod": "rm -rf dist/ && NODE_ENV=production webpack --mode production",
    "app:build:stage": "rm -rf dist/ && NODE_ENV=staging webpack --mode production",
    "ext:start:dev": "NODE_ENV=development NODE_APP_INSTANCE=extension webpack serve --mode development",
    "ext:build:prod": "NODE_ENV=production NODE_APP_INSTANCE=extension webpack --mode production",
    "demo:start:dev": "NODE_ENV=development NODE_APP_INSTANCE=demo webpack serve --mode development",
    "demo:build:stage": "rm -rf dist-demo/ && NODE_ENV=staging NODE_APP_INSTANCE=demo webpack --mode production",
    "demo:build:prod": "rm -rf dist-demo/ && NODE_ENV=production NODE_APP_INSTANCE=demo webpack --mode production",
    "demo:deploy:stage": "firebase deploy --only hosting:stage",
    "demo:deploy:prod": "firebase deploy --only hosting:app",
    "demo:emulate:stage": "firebase emulators:start --only hosting:stage",
    "demo:emulate:prod": "firebase emulators:start --only hosting:app",
    "zip:ext": "cd ext-dist/ && zip build index.html index.js config.html panel.html streamer.html",
    "cleanup:ext": "cd ext-dist/ && rm *.html *.js *.txt *.zip",
    "gql:schema": "get-graphql-schema https://api.thegraph.com/subgraphs/name/h0ppx/two > src/lib/graphql/clipit.graphql",
    "gql:types": "graphql-codegen",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier -w .",
    "fb:login": "firebase login",
    "fb:init": "firebase init",
    "analyze": "NODE_ENV=development webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist/"
  },
  "author": "h0ppx",
  "license": "ISC",
