    "@types/twitch-ext": "^1.24.4",
    "axios": "^0.21.1",
    "dataloader": "^2.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.4.4",
